-- MAIN
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")
MainSection:NewButton("Rainbow Hand", "Gives Rainbow Hand for 1m", function()
    Game.ReplicatedStorage.RemoteEvents.BuyItemCash:FireServer
"Hand Rainbow"
end)
MainSection:NewButton("F", "Gives F", function()
    Game.ReplicatedStorage.RemoteEvents.BuyItemCash:FireServer
"F"
end)
MainSection:NewButton("Harambe", "Free NFT", function()
    Game.ReplicatedStorage.RemoteEvents.BuyItemCash:FireServer
"Box Toy Harambe"
end)
