local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Hell Hub | v1.3.6.3", "Ocean")

-- MAIN
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")
MainSection:NewButton("Rainbow Hand", "Gives Rainbow Hand for 1m", function()
    Game.ReplicatedStorage.RemoteEvents.BuyItemCash:FireServer
"Hand Rainbow"
end)
MainSection:NewButton("F", "Gives F", function()
    Game.ReplicatedStorage.RemoteEvents.BuyItemCash:FireServer
"F"
end)
MainSection:NewButton("Harambe", "Free NFT", function()
    Game.ReplicatedStorage.RemoteEvents.BuyItemCash:FireServer
"Box Toy Harambe"
end)
MainSection:NewButton("Dupe", "dupes (piano needed)", function()
while wait() 
 do 
Game.ReplicatedStorage.RemoteEvents.Equip:FireServer
"Piano"
    anything = true 
  end
  end)

local AutoBuy = Window:NewTab("AutoBuy")
local AutoBuySection = AutoBuy:NewSection("AutoBuy")
AutoBuySection:NewToggle("Auto Buy F", "inf buys F", function(state)
    if state then
        getgenv().l = true
while getgenv().l do
wait(1)
local args = {
    [1] = "F"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
end
    else
        getgenv().l = false
    end
end)
AutoBuySection:NewToggle("Auto Buy RB Hand", "inf buys RB Hand", function(state)
    if state then
        getgenv().l = true
while getgenv().l do
wait(1)
local args = {
    [1] = "Hand Rainbow"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
end
    else
        getgenv().l = false
    end
end)
AutoBuySection:NewToggle("Auto Buy Harambe", "inf buys Harambe", function(state)
    if state then
        getgenv().l = true
while getgenv().l do
wait(1)
local args = {
    [1] = "Box Toy Harambe"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
end
    else
        getgenv().l = false
    end
end)
local FakeDrop = Window:NewTab("FakeDrop")
local FakeDropSection = FakeDrop:NewSection("FakeDrop")
FakeDropSection:NewButton("Fake Drops Item", "USE reset after scam", function()
local plr = game.Players.LocalPlayer

local chr = plr.Character

chr.RightHand:Destroy()
end)
FakeDropSection:NewButton("Reset", "This is obvious what this does", function()
local plr = game.Players.LocalPlayer

local chr = plr.Character

chr.Head:Destroy()


