local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("Hell Hub | v1.1.6.3", "Ocean")

-- MAIN
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")
MainSection:NewButton("Rainbow Hand", "Gives Rainbow Hand for 1m", function()
    Game.ReplicatedStorage.RemoteEvents.BuyItemCash:FireServer
"Hand Rainbow"
end)
MainSection:NewButton("F", "Gives F", function()
    Game.ReplicatedStorage.RemoteEvents.BuyItemCash:FireServer
"F"
end)
MainSection:NewButton("Harambe", "Free NFT", function()
    Game.ReplicatedStorage.RemoteEvents.BuyItemCash:FireServer
"Box Toy Harambe"
end)
  
 

local AutoBuy = Window:NewTab("AutoBuy")
local AutoBuySection = AutoBuy:NewSection("AutoBuy")
AutoBuySection:NewToggle("Auto Buy F", "inf buys F", function(state)
    if state then
        getgenv().l = true
while getgenv().l do
wait(1)
local args = {
    [1] = "F"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
end
    else
        getgenv().l = false
    end
end)
AutoBuySection:NewToggle("Auto Buy RB Hand", "inf buys RB Hand", function(state)
    if state then
        getgenv().l = true
while getgenv().l do
wait(1)
local args = {
    [1] = "Hand Rainbow"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
end
    else
        getgenv().l = false
    end
end)
AutoBuySection:NewToggle("Auto Buy Harambe", "inf buys Harambe", function(state)
    if state then
        getgenv().l = true
while getgenv().l do
wait(1)
local args = {
    [1] = "Box Toy Harambe"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
end
    else
        getgenv().l = false
    end
end)
local Dupe = Window:NewTab("Dupe")
local DupeSection = Dupe:NewSection("Dupe")
DupeSection:NewButton("Dupe", "dupes (piano needed)", function()
while wait() 
 do 
Game.ReplicatedStorage.RemoteEvents.Equip:FireServer
"Piano"
    anything = true 
  end
  end)
  DupeSection:NewButton("SafePlace", "TP's to safe place.", function()
  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(41.3862267, 61.8797607, -304.975037, -0.999932826, -1.85789284e-09, -0.0115917446, -1.92988336e-09, 1, 6.19931972e-09, 0.0115917446, 6.22127416e-09, -0.999932826)
  end)
  
local Player = Window:NewTab("Player")
local PlayerSection = Player:NewSection("Player")
PlayerSection:NewToggle("Super-Human", "120 spd 120 jump", function(state)
if state then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 120
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 120
else
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    end
end)
   
local FakeDrop = Window:NewTab("FakeDrop")
local FakeDropSection = FakeDrop:NewSection("FakeDrop")
FakeDropSection:NewButton("Fake Drops Item", "USE reset after scam", function()
local plr = game.Players.LocalPlayer

local chr = plr.Character

chr.RightHand:Destroy()
end)
FakeDropSection:NewButton("Reset", "This is obvious what this does", function()
local plr = game.Players.LocalPlayer

local chr = plr.Character

chr.Head:Destroy()
end)
local Credits = Window:NewTab("Credits")
local Credits = Credits:NewSection("Snakez#2695")
local Updates = Window:NewTab("Updates")
local Updates = Updates:NewSection("FakeDrop")





